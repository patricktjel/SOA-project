<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wsdl:definitions name="orchestrator"
	targetNamespace="http://www.example.org/orchestrator"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
	xmlns:tns="http://www.example.org/orchestrator" 
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" 
	xmlns:xsd="http://www.w3.org/2001/XMLSchema">

	<wsdl:message name="requestForDeliveryResponse">
		<wsdl:part name="expectedDeliveryDate" type="xsd:dateTime"/>
		<wsdl:part name="price" type="xsd:double"/>
	</wsdl:message>
	
	<wsdl:message name="trackingReminder">
		<wsdl:part name="iceSkateID" type="xsd:integer"/>
		<wsdl:part name="expectedDeliveryDate" type="xsd:dateTime"/>
	</wsdl:message>
	
	<wsdl:portType name="OrchestratorPortType">
		<wsdl:operation name="DeliveryResponse">
			<wsdl:input message="tns:requestForDeliveryResponse"/>
		</wsdl:operation>
		<wsdl:operation name="TrackingReminder">
			<wsdl:input message="tns:trackingReminder"/>
		</wsdl:operation>
	</wsdl:portType>
	
	<wsdl:binding name="orchestratorBinding" type="tns:OrchestratorPortType">
		<soap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>
		<wsdl:operation name="DeliveryResponse">
			<soap:operation style="rpc"/>
			<wsdl:input>
				<soap:body use="literal" namespace="http://www.example.org/orchestrator"/>
			</wsdl:input>
		</wsdl:operation>
		<wsdl:operation name="TrackingReminder">
			<soap:operation style="rpc"/>
			<wsdl:input>
				<soap:body use="literal" namespace="http://www.example.org/orchestrator"/>
			</wsdl:input>
		</wsdl:operation>
	</wsdl:binding>

	<wsdl:service name="OrchestratorService">
		<wsdl:port name="orchestratorPort" binding="tns:orchestratorBinding">
			<soap:address location="http://localhost:8080/SOA-project/services/orchestratorService"/>
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>